{
  "//": "@todo-wordpress",
  "name": "xm_starter_wordpress",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xmmedia/starter_wordpress"
  },
  "license": "MIT",
  "//private": true,
  "scripts": {
    "dev": "encore dev",
    "dev-server": "encore dev-server --hot --https --allowed-hosts all --port 9034",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "cypress open",
    "lint:js": "vue-cli-service lint --ext .js,.vue public/app/themes/default/js *.js --no-fix",
    "lint:js:fix": "vue-cli-service lint --ext .js,.vue public/app/themes/default/js *.js",
    "lint:css": "stylelint public/app/themes/default/css/**/*.scss",
    "lint:css:fix": "stylelint public/app/themes/default/css/**/*.scss --fix",
    "watch": "encore dev --watch",
    "build": "encore production",
    "stats": "encore production --profile --json > webpack-stats.json",
    "audit:moderate": "audit-ci --moderate --config audit-ci.json --report-type summary",
    "audit:high": "audit-ci --high --config audit-ci.json --report-type summary",
    "browserlist": "browserslist"
  },
  "dependencies": {
    "core-js": "^3.4.3",
    "jquery": "^3.4.1",
    "portal-vue": "^2.1.3",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@symfony/webpack-encore": "^4.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-e2e-cypress": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-pwa": "^4.0.5",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/test-utils": "^1.0.0",
    "audit-ci": "^6.0.0",
    "autoprefixer": "^10.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.2",
    "css-loader": "^6.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^6.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-vue": "^7.0.1",
    "file-loader": "^6.0.0",
    "glob-all": "^3.1.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^7.0.0",
    "sass-embedded": "^1.0.0",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.0.0",
    "svgo": "^2.2.2",
    "svgo-loader": "^3.0.0",
    "svgxuse": "^1.2.0",
    "tailwindcss": "^3.0",
    "url-loader": "^4.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^5.0.0"
  }
}
